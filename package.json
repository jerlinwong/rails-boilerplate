    "lint:js": "tsc && eslint . --ext .js,.jsx,.ts,.tsx,.json --max-warnings=0 --fix",
    "lint:rb": "bundle exec rubocop -a",
    "postinstall": "husky install"
  },
  "devDependencies": {
    "@babel/core": "^7.22.20",
    "@babel/eslint-parser": "^7.22.15",
    "@babel/plugin-transform-react-jsx": "^7.22.15",
